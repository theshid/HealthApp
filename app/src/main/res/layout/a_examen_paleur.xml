                    android:layout_width="330dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/editfield"
                    android:clickable="true"
                    android:drawablePadding="5dp"
                    android:ems="10"
                    android:focusable="true"
